    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name=".StudyApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".firstcode.Main2Activity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Home" />
        <activity android:name=".viewstub.ViewStubActivity" />
        <activity android:name=".lambda.LambdaActivity" />
        <activity android:name=".recyclerview.RecyclerViewActivity"/>
        <activity android:name=".net.NetActivity"/>
        <activity android:name=".recyclerview.ListViewActivity"/>
        <activity android:name=".bluetooth.BluetoothActivity"/>
        <activity android:name=".testuses.TestActivity"/>
        <activity android:name=".ble.BLEActivity"/>
        <activity android:name=".recyclerview.RecyclerViewActivityOne"/>
        <activity android:name=".recyclerview.brvahdemo.BaseUseActivity"/>
        <activity android:name=".sensors.SensorsActivity"/>
        <activity android:name=".sensor.SensorActivity" />
        <activity android:name=".textview.TextViewActivity" />
